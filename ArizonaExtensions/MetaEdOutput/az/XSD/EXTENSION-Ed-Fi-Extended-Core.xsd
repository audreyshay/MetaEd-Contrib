<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/0310" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/0310" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 3.1.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-CalendarExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Calendar">
        <xs:sequence>
          <xs:element name="CalendarLocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>The LEA id of the district submitting the track</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseOfferingExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CourseOffering">
        <xs:sequence>
          <xs:element name="EndOfCourseAssessmentCode" type="EXTENSION-EndOfCourseAssessmentCodeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end of course assesment code. Submitted for courses that end with Assessment testing</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-EndOfCourseAssessmentCodeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseOfferingCourseSchedule" type="EXTENSION-CourseOfferingCourseSchedule" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>CourseSchedule for given course</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SectionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Section">
        <xs:sequence>
          <xs:element name="SectionExternalProviderTeacher" type="EXTENSION-SectionExternalProviderTeacher" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>A collection fo external teachers assigned to a section</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SectionGradeLevel" type="EXTENSION-SectionGradeLevel" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The grade level associated with the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TimetableDayIdentifier" type="EXTENSION-TimetableDayIdentifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Report the days on which the course/section meets. MTWRF if meets
                              every day, MWF if the course/section meets every other day. If
                              the course is on a block schedule, use AB.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Note" type="EXTENSION-Note" minOccurs="0">
            <xs:annotation>
              <xs:documentation>This is a free field that may contain up to 250 characters.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentDropOutRecoveryProgramMonthlyUpdate">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentDropOutRecoveryProgramAssociationReference" type="EXTENSION-StudentDropOutRecoveryProgramAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The drop out recovery program that the monthly update is reported</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RevisedWrittenLearningPlanDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Revised Learning Plan Date is used when plan requires changes
                                to  the initial written plan.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Month" type="EXTENSION-MonthDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>the month for which the progress update is reported</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-MonthDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SatisfactoryProgress" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates whether student maintained satisfactory progress during
                                the month prior to this record. This data must be submitted each
                                month that the student is enrolled in the DRP regardless of the
                                student's progress. This field is nullable.  If no data is submitted,
                                it will be NULL.  If the LEA/School submits data it will be a
                                0 or a 1 where 0 = Unsatisfactory and 1 = Satisfactory</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentNeed">
    <xs:annotation>
      <xs:documentation>This entity represents a need for a student</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>A unique alphanumeric code assigned to a student</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>The identifier assigned to an education agency reporting the need</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeed" type="EXTENSION-StudentNeedDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The student need descriptor code</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StudentNeedDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeedEntryDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date the student began the state of having the Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeedExitDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the student exited the state of having the Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrimaryStudentNeedIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the SPED Need being serviced is the Primary Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrimaryNightTimeResidence" type="EXTENSION-PrimaryNightTimeResidenceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Primary Night time residence descriptor associated with student
                                who have been designated as Homeless.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-PrimaryNightTimeResidenceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentProgramAttendanceEventExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentProgramAttendanceEvent">
        <xs:sequence>
          <xs:element name="TimeLog" type="EXTENSION-TimeLog" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This event entity adds the time component to a student program attendance participating</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAttendanceEventExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSchoolAttendanceEvent">
        <xs:sequence>
          <xs:element name="InstructionalMinutes" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time, in minutes, that the student received instruction during the reported attendance period and the 'In Attendance' code is used</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Descriptors =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AbsenceAmountDescriptor">
    <xs:annotation>
      <xs:documentation>Absence Amounts must be less than or equal to the  Student Membership
                        FTE, since absences are measured in quarter day increments.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseEntryDescriptor">
    <xs:annotation>
      <xs:documentation>The way the student entered this course, either as a
                          new enrollee or as a transfer from another section at the same
                          school</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseExitDescriptor">
    <xs:annotation>
      <xs:documentation>The type of exit the student had from this course section.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-EducationLevelDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines education level of a staff who is asscoiated with education organization.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-EndOfCourseAssessmentCodeDescriptor">
    <xs:annotation>
      <xs:documentation>Submitted for courses that end with Assessment testing</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ExitWithdrawReasonDescriptor">
    <xs:annotation>
      <xs:documentation>Withdrawal Reasons are a secondary reason for the withdrawal (the
                        primary reason being the Withdrawal Activity Code). Withdrawal
                        Reasons are optional, and will not apply to the great majority
                        of students who withdraw from school prior to the last scheduled
                        day of session.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipFTEDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor describes the full-time equivalency codes</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipResponsibilityDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines types of responsibility a local education agency may have for a student school association(e.g., funding).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipTypeDescriptor">
    <xs:annotation>
      <xs:documentation>Membership Type identifies whether a school has primary responsibility
                        for managing a specific student's curriculum or not. The 'P' code
                        value will allow the reporting of students who are enrolled in
                        a SPED program but not enrolled in a school.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MonthDescriptor">
    <xs:annotation>
      <xs:documentation>The month number descriptor</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-NumberOfInstructionalDaysInWeekDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines the number of instructional days for a week in calendar track (e.g., three days per week, fours days per week, five days per week).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-PrimaryNightTimeResidenceDescriptor">
    <xs:annotation>
      <xs:documentation>The Primary Nighttime residence is a field associated with students
                        who have been designated as Homeless.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SpecialEnrollmentDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor describes special enrollment codes</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentNeedCategoryDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines a category of a student need.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentNeedDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines a characteristic of a student need</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType">
        <xs:sequence>
          <xs:element name="StudentNeedCategory" type="EXTENSION-StudentNeedCategoryDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation></xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StudentNeedCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-TuitionPayerDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines the tuition payer codes</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StaffEducationOrganizationEmploymentAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StaffEducationOrganizationEmploymentAssociation">
        <xs:sequence>
          <xs:element name="EducationLevel" type="EXTENSION-EducationLevelDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>It defines the staff education level</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-EducationLevelDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Benefits" type="EXTENSION-Benefits" minOccurs="0">
            <xs:annotation>
              <xs:documentation>It is benefit percentage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Salary" type="Currency">
            <xs:annotation>
              <xs:documentation>Salary defined for a staff.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FullTimeEquivalencyOther" type="Percent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The ratio between the hours of work expected in a position and the hours of work normally expected in a full-time position in the same setting.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PSP" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Purchased Services Personnel (PSP) employees.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentDropOutRecoveryProgramAssociation">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="GeneralStudentProgramAssociation">
        <xs:sequence>
          <xs:element name="WrittenLearningPlanDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date the initial Written Learning Plan was approved. Note: A new Written
                                Learning Plan date indicating preparation of a new plan must be submitted
                                each academic (fiscal) year.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSchoolAssociation">
        <xs:sequence>
          <xs:element name="ExitWithdrawReason" type="EXTENSION-ExitWithdrawReasonDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Withdrawal Reasons are a secondary reason for the withdrawal (the
                                primary reason being the Withdrawal Activity Code). Withdrawal Reasons
                                are optional, and will not apply to the great majority of students
                                who withdraw from school prior to the last scheduled day of session</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ExitWithdrawReasonDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="MembershipType" type="EXTENSION-MembershipTypeDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Membership Type identifies whether a school has primary responsibility
                                for managing a specific student's curriculum or not. The 'P'code value
                                will allow the reporting of students who are enrolled in a SPED program
                                but not enrolled in a school.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-MembershipTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentSchoolAssociationLocalEducationAgency" type="EXTENSION-StudentSchoolAssociationLocalEducationAgency" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This association represents any special enrollment characteristics of the student school association</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentSchoolAssociationMembershipFTE" type="EXTENSION-StudentSchoolAssociationMembershipFTE" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This association represents the full-time equivalency of the student school association</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentSchoolAssociationTuitionPayer" type="EXTENSION-StudentSchoolAssociationTuitionPayer" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This association represents the tuition payer codes of the student school association</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentSchoolAssociationSpecialEnrollment" type="EXTENSION-StudentSchoolAssociationSpecialEnrollment" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This association represents any special enrollment characteristics of the student school association</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSectionAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSectionAssociation">
        <xs:sequence>
          <xs:element name="Note" type="EXTENSION-Note" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Any Clarifying information for this course section</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseEntry" type="EXTENSION-CourseEntryDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The way the student entered this course section, either as a new
                              enrollee or as a transfer from another section at the same school</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CourseEntryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseExit" type="EXTENSION-CourseExitDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The type of exit the student had from this course section</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CourseExitDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="DualCredit" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>A flag indicating if this student is earning Dual Credit in a Dual
                              Credit Course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConcurrentEnrollment" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>A flag indicating if this student is concurrently enrolled in a
                              course that offers concurrent enrollment</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSpecialEducationProgramAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSpecialEducationProgramAssociation">
        <xs:sequence>
          <xs:element name="MainSPEDSchool" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A flag indicating if this is Primary school for SPED concurrency or not</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Extended Reference Types =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentDropOutRecoveryProgramAssociationIdentityType">
    <xs:annotation>
