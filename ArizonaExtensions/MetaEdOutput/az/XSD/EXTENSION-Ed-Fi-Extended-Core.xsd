    <xs:annotation>
      <xs:documentation>This is a free field that may contain up to 250 characters.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Simple</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="250" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-ProviderTeacherFirstName">
    <xs:annotation>
      <xs:documentation>If the course is being provided by an external provider, the first
                        or given name of the provider's primary instructor. Required if
                        Provider Teacher 1 Last Name is provided.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Simple</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>
